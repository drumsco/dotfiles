<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>edit-server</string>
        <key>ProgramArguments</key>
        <array>
            <string>sh</string>
            <string>-c</string>
            <string>
                export EDIT_SERVER_EDITOR="/Applications/MacVim.app/Contents/MacOS/gvim -f";
                python
                $HOME/src/github.com/tmsanrinsha/edit-server/edit-server
            </string>
        </array>
        <key>OnDemand</key>
        <false/>
        <key>StandardOutPath</key>
        <string>/Users/tmsanrinsha/logs/edit-server.log</string>
        <key>StandardErrorPath</key>
        <string>/Users/tmsanrinsha/logs/edit-server-err.log</string>
    </dict>
</plist>
