{
    "Style": {
        "Basic": ["on_default", "default"],
        "SavedSelection": ["red"],
        "Selected": ["on_blue", "white", "bold"],
        "Query": ["yellow"],
        "Matched": ["green", "bold"]
    },
    "CustomFilter": {
        "Migemo": {
            "Cmd": "migemogrep",
            "BufferThreshold": 10,
            "Args": [ "$QUERY" ]
        }
    },
    "StickySelection": true,
    "Keymap": {
        "C-e": "peco.ExecuteCommand.Notepad",
        "C-x,C-c": "peco.Cancel"
    },
    "Command": [
        {
            "Name": "Notepad",
            "Args": ["mvim", "$LINE"],
            "Spawn": false
        }
    ]
}
