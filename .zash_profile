if [ -d ~/.profile.d ]; then
    for i in ~/.profile.d/*.sh ; do
        if [ -r "$i" ]; then
            if [ "${-#*i}" != "$-" ]; then
                . $i
            else
                . "$i" >/dev/null 2>&1
            fi
        fi
    done
fi

git --git-dir=$HOME/git/tmsanrinsha/dotfiles/.git --no-pager pull 1>/dev/null &
vim -N -u NONE -i NONE -V1 -e -s --cmd "source ~/.vimrc" --cmd NeoBundleInstall! --cmd qall! 1>/dev/null 2>&1 &

if [ -f ~/.zash_profile.local ];then
    . ~/.zash_profile.local
fi

